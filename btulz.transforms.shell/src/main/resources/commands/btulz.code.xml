<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:CommandBuilder Name="btulz.code" Description="生成代码"
	xmlns:ns2="http://colorcoding.org/btulz/shell/commands">
	<Item Name="main" Content="java" />
	<Item Name="jar" Content="-jar ${WORK_FOLDER}btulz.transforms.core-0.1.0.jar"
		Description="使用jar包" />
	<Item Name="sub" Content="code" Description="子命令" />
	<Item Content="-TemplateFolder=${Value}" Description="使用的模板">
		<ValidValues>
			<ValidValue Value="eclipse/ibas_classic" />
		</ValidValues>
	</Item>
	<Item Content="-OutputFolder=${Value}" Description="输出目录" />
	<Item Content="-GroupId=${Value}" Description="组标记" />
	<Item Content="-ArtifactId=${Value}" Description="项目标记" />
	<Item Content="-ProjectVersion=${Value}" Description="项目标记" />
	<Item Content="-ProjectUrl=${Value}" Description="项目地址" />
	<Item Content="-Domains=${Value}" Description="模型文件" />
	<Item Content="-Release" Description="释放资源" />
	<Item Content="-Parameters=[{${Value}}]" Description="其他参数">
		<Items>
			<Item
				Content="{&quot;name&quot;:&quot;Company&quot;,&quot;value&quot;:&quot;${Value}&quot;}"
				Description="公司标识" />
			<Item
				Content=",{&quot;name&quot;:&quot;ibasVersion&quot;,&quot;value&quot;:&quot;${Value}&quot;}"
				Description="ibas版本" />
			<Item
				Content=",{&quot;name&quot;:&quot;jerseyVersion&quot;,&quot;value&quot;:&quot;${Value}&quot;}"
				Description="jersey版本" />
		</Items>
	</Item>
</ns2:CommandBuilder>